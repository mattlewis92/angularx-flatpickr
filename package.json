{
  "name": "angularx-flatpickr",
  "version": "6.5.2",
  "description": "An angular 5.0+ wrapper for flatpickr",
  "scripts": {
    "start": "concurrently --raw \"webpack-dev-server --open\" \"npm run test:watch\"",
    "build:demo": "webpack",
    "build:dist": "ng-packagr -p package.json",
    "build:clean": "del-cli dist",
    "test": "TZ=Europe/London karma start --single-run && npm run build:dist && npm run build:clean",
    "test:watch": "TZ=Europe/London karma start --auto-watch",
    "commit": "git-cz",
    "compodoc": "compodoc -p tsconfig-compodoc.json -d docs --disableGraph --disableCoverage --disablePrivate --disableInternal --disableLifeCycleHooks --disableProtected",
    "gh-pages": "git checkout gh-pages && git merge master --no-edit --no-ff && npm run build:demo && npm run compodoc && git add . && git commit -m \"chore: build demo and docs\" && git push && git checkout master",
    "copyfiles": "copyfiles CHANGELOG.md dist",
    "prerelease": "npm test",
    "release:git": "standard-version && git push --follow-tags origin master",
    "release:npm": "npm run build:dist && npm run copyfiles && npm publish dist",
    "release": "npm run release:git && npm run release:npm",
    "postrelease": "npm run build:clean && npm run gh-pages",
    "commitmsg": "validate-commit-msg",
    "codecov": "cat coverage/lcov.info | codecov",
    "precommit": "pretty-quick --staged"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlewis92/angularx-flatpickr.git"
  },
  "keywords": [
    "angular2",
    "angular",
    "angular5",
    "flatpickr",
    "datepicker"
  ],
  "author": "Matt Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlewis92/angularx-flatpickr/issues"
  },
  "homepage": "https://github.com/mattlewis92/angularx-flatpickr#readme",
  "devDependencies": {
    "@angular/common": "^6.0.7",
    "@angular/compiler": "^6.0.7",
    "@angular/compiler-cli": "^6.0.7",
    "@angular/core": "^6.0.7",
    "@angular/forms": "^6.0.7",
    "@angular/language-service": "^6.0.7",
    "@angular/platform-browser": "^6.0.7",
    "@angular/platform-browser-dynamic": "^6.0.7",
    "@compodoc/compodoc": "^1.1.3",
    "@types/chai": "^4.1.4",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/mocha": "^5.2.4",
    "@types/node": "^10.5.1",
    "@types/sinon": "^4.3.3",
    "@types/sinon-chai": "^3.2.0",
    "@types/webpack": "^4.4.4",
    "chai": "^4.1.2",
    "codecov": "^3.0.2",
    "codelyzer": "^4.4.2",
    "commitizen": "^2.10.1",
    "concurrently": "^3.6.0",
    "copyfiles": "^2.0.0",
    "core-js": "^2.5.7",
    "css-loader": "^0.28.11",
    "cz-conventional-changelog": "^2.1.0",
    "del-cli": "^1.1.0",
    "flatpickr": "^4.6.3",
    "fork-ts-checker-webpack-plugin": "^0.4.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "ng-packagr": "^3.0.3",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0",
    "rxjs": "^6.2.1",
    "sinon": "^6.0.1",
    "sinon-chai": "^3.2.0",
    "standard-version": "^4.4.0",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-mwl": "^0.5.2",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.7.2",
    "validate-commit-msg": "^2.12.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "zone.js": "^0.8.26"
  },
  "peerDependencies": {
    "@angular/core": ">=5.0.0",
    "@angular/forms": ">=5.0.0",
    "flatpickr": "^4.5.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
